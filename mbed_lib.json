{
    "name": "mbed-lvgl",
    "config": {
	"horizontal_res": {
	    "help": "Maximal horizontal resolution to support by the library.",
	    "macro_name": "LV_HOR_RES_MAX",
	    "value": 480
	},
	"vertical_res": {
	    "help": "Maximal vertical resolution to support by the library.",
	    "macro_name": "LV_VER_RES_MAX",
	    "value": 320
	},
	"color_depth": {
	    "help": "Color depth of display [1 /8 (RGB233) / 16 (RGB565) / 32 (ARGB8888)]",
	    "accepted_values": [1, 8, 16, 32]
	    "macro_name": "LV_COLOR_DEPTH",
	    "value": 16
	},
	"color_16_swap": {
	    "help": "Swap the 2 bytes of RGB565 color.",
	    "macro_name": "LV_COLOR_16_SWAP",
	    "value": 0
	},
	"screen_transparency": {
	    "help": "Enable screen transparency. Requires LV_COLOR_DEPTH = 32",
	    "macro_name": "LV_COLOR_SCREEN_TRANSP",
	    "value": 0
	},
	"transparent_color": {
	    "help": "Image pixels with this color will not be drawn (with chroma keying)",
	    "macro_name": "LV_COLOR_TRANSP",
	    "value": "LV_COLOR_LIME"
	},
	"antialias": {
	    "help": "Enable/disable antialiasing",
	    "macro_name": "LV_ANTIALIAS",
	    "value": 1
